<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Login">

    <EditText
            android:text="Password"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPassword"
            android:ems="10"
            tools:layout_editor_absoluteY="229dp" tools:layout_editor_absoluteX="72dp" android:id="@+id/editText"/>
    <EditText
            android:text="Usuario"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textEmailAddress"
            android:ems="10"
            tools:layout_editor_absoluteY="162dp" tools:layout_editor_absoluteX="72dp" android:id="@+id/editText2"/>
    <ImageButton
            android:layout_width="wrap_content"
            android:backgroundTint="transparent"
            android:layout_height="wrap_content" app:srcCompat="@drawable/boton_entrar_login"
            tools:layout_editor_absoluteY="313dp" tools:layout_editor_absoluteX="16dp" android:id="@+id/imageButton"/>
    <TextView
            android:text="Nuevo?"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="400dp"
            tools:layout_editor_absoluteX="72dp" android:id="@+id/textView4"/>
    <TextView
            android:text="REGISTRARSE"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="400dp"
            tools:layout_editor_absoluteX="229dp" android:id="@+id/textView6"/>
    <ImageView
            android:layout_width="568dp"
            android:layout_height="409dp" app:srcCompat="@drawable/fondo_abajo"
            tools:layout_editor_absoluteX="-134dp" android:id="@+id/imageView5" android:layout_marginTop="96dp"
            app:layout_constraintTop_toBottomOf="@+id/imageButton"/>
</android.support.constraint.ConstraintLayout>